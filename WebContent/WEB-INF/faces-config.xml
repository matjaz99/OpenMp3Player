<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<component>
		<component-type>omp3p.multiTagComponent</component-type>
		<component-class>si.matjazcerkvenik.openmp3player.web.comp.MultiTagComponent</component-class>
	</component>
	<component>
		<component-type>omp3p.singleTagComponent</component-type>
		<component-class>si.matjazcerkvenik.openmp3player.web.comp.SingleTagComponent</component-class>
	</component>
	<managed-bean>
		<managed-bean-name>playerBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.PlayerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>playlistBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.PlaylistBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>playlistsBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.PlaylistsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>addPlaylistBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.AddPlaylistBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>songBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.SongBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>addTagBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.AddTagBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>queueBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.QueueBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>settingsBean</managed-bean-name>
		<managed-bean-class>si.matjazcerkvenik.openmp3player.web.SettingsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>about</from-outcome>
			<to-view-id>/about.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>settings</from-outcome>
			<to-view-id>/settings.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>playlists</from-outcome>
			<to-view-id>/playlists.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>addPlaylist</from-outcome>
			<to-view-id>/addPlaylist.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>queue</from-outcome>
			<to-view-id>/queue.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>song</from-outcome>
			<to-view-id>/song.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<render-kit>
		<renderer>
			<component-family>omp3p.multiTag</component-family>
			<renderer-type>omp3p.multiTagRenderer</renderer-type>
			<renderer-class>si.matjazcerkvenik.openmp3player.web.comp.MultiTagRenderer</renderer-class>
		</renderer>
	</render-kit>
	<validator>
		<validator-id>playlistValidator</validator-id>
		<validator-class>si.matjazcerkvenik.openmp3player.web.validators.PlaylistValidator</validator-class>
	</validator>
	<validator>
		<validator-id>directoryValidator</validator-id>
		<validator-class>si.matjazcerkvenik.openmp3player.web.validators.DirectoryValidator</validator-class>
	</validator>
	<validator>
		<validator-id>tagValidator</validator-id>
		<validator-class>si.matjazcerkvenik.openmp3player.web.validators.TagValidator</validator-class>
	</validator>
	<application>
		<locale-config>
			<default-locale>en_US</default-locale>
			<supported-locale>en_US</supported-locale>
			<supported-locale>es_ES</supported-locale>
		</locale-config>
		<message-bundle>si.matjazcerkvenik.openmp3player.resources.omp3p-resources</message-bundle>
	</application>
</faces-config>
